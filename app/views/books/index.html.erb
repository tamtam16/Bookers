<h1>Books</h1>
<table>
  <thead>
    <tr>
      <th>Title</tr>
      <th>Body</tr>
      <th colspan="3"></th>
    </tr>
  </table>

<h1>New book</h1>

<%= form_with model: @book do |f| %>

   
 <h4>Title</h4>
 <%= f.text_field :title %>

 <h4>Body</h4>
 <%= f.text_area :body %>

 <%= f.submit 'Create Book' %>


<% end %>